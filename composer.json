{
    "name": "sanmisha/cartly",
    "description": "Slim + Twig + Alpine ecommerce",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Cartly\\": "src/"
        }
    },
    "scripts": {
        "migrate": "php cartly migrate",
        "seed:dev": "php cartly db:seed",
        "seed:demo": "php cartly db:seed --demo",
        "start": "bash scripts/start.sh",
        "serve": "php cartly serve"
    },
    "bin": [
        "cartly"
    ],
    "authors": [
        {
            "name": "Sanjeev Divekar",
            "email": "sanjeev@sanmisha.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "slim/slim": "4.*",
        "slim/twig-view": "^3.4",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^12.46",
        "slim/psr7": "^1.8",
        "illuminate/events": "^12.46",
        "illuminate/filesystem": "^12.46",
        "illuminate/support": "^12.46",
        "firebase/php-jwt": "^7.0",
        "symfony/rate-limiter": "^7.4",
        "monolog/monolog": "^3.10",
        "vlucas/valitron": "^1.4.11",
        "phpmailer/phpmailer": "^6.9",
        "php-di/php-di": "^7.0"
    }
}
